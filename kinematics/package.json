{
  "name": "kinematics",
  "displayName": "Kinematics",
  "description": "Kinematics language support",
  "version": "0.9.0",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:kinematics"
  ],
  "main": "lib/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "kinematics",
        "aliases": [
          "Kinematics",
          "kinematics"
        ],
        "extensions": [
          ".kin"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kinematics",
        "scopeName": "source.kin",
        "path": "./syntaxes/kinematics.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "kinematics.openPreview",
        "title": "Open kinematics preview window",
        "category": "Kinematics",
        "when": "editorLangId == xacro"
      }
    ]
  },
  "scripts": {
    "build-viewer": "webpack --mode=production --config ./src/urdf/webpack.config.js",
    "antlr4ts": "antlr4ts -visitor src/parser/DebugInternalKinematics.g",
    "build": "tsc -p . && npm run build-viewer"
  },
  "devDependencies": {
    "@types/vscode": "^1.49.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.5.5",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@types/roslib": "^1.1.8",
    "@types/three": "^0.137.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "assert": "^2.0.0",
    "grunt-cli": "^1.4.3",
    "resolve-cwd": "^3.0.0",
    "roslib": "latest",
    "three": "latest",
    "util": "^0.12.4",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^7.0.0",
    "webpack": "^5.76.1"
  }
}
